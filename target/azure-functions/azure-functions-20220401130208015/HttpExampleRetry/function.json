{
  "scriptFile" : "../azure-functions-1.0-SNAPSHOT.jar",
  "entryPoint" : "solide.Function.HttpExampleRetry",
  "bindings" : [ {
    "type" : "httpTrigger",
    "direction" : "in",
    "name" : "req",
    "methods" : [ "GET", "POST" ],
    "authLevel" : "ANONYMOUS"
  }, {
    "type" : "http",
    "direction" : "out",
    "name" : "$return"
  } ],
  "retry" : {
    "strategy" : "fixedDelay",
    "maxRetryCount" : 3,
    "delayInterval" : "00:00:05"
  }
}